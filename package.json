{
  "name": "boots-js",
  "version": "1.0.0",
  "description": "BootsJS is a library dedicated to extending the capabilities of native JavaScript, aiming to address common data structures, methods, and frequently used algorithms that are not natively supported in JavaScript.",
  "main": "index.js",
  "scripts": {
    "build": "npx rollup -c",
    "format": "prettier --write src/",
    "test": "npx jest",
    "doc": "npx typedoc",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JunLiangWangX/BootsJs.git"
  },
  "keywords": [
    "boots-js",
    "javascript",
    "native",
    "library",
    "lib",
    "node",
    "browser"
  ],
  "author": "JunLiangWang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JunLiangWangX/BootsJs/issues"
  },
  "homepage": "https://junliangwangx.github.io/BootsJS/",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
